{
  "name": "@liuli-util/cmd-shim",
  "version": "0.3.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "bin": "./bin.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext",
    "dev": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext --watch",
    "lint": "eslint --fix src/**",
    "setup": "pnpm build",
    "start": "tsx src/bin.ts",
    "test": "vitest run"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@liuli-util/async": "workspace:^",
    "@liuli-util/fs-extra": "workspace:^",
    "cmd-shim": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "ast-types": "^0.14.2",
    "recast": "^0.23.2",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "type-fest": "^3.10.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

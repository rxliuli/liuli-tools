{
  "name": "create-liuli",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "setup": "pnpm build",
    "build": "pnpm tsup src/bin.ts --format esm --target esnext",

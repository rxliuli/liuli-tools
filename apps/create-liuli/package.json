{
  "name": "create-liuli",
  "version": "0.3.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "bin": "./bin.js",
  "files": [
    "bin.js",
    "dist",
    "packages"
  ],
  "scripts": {
    "build": "pnpm tsup src/bin.ts --format esm --target esnext",
    "setup": "pnpm build",
    "start": "tsx src/bin.ts",
    "test": "vitest run"
  },
  "sideEffects": false,
  "dependencies": {
    "commander": "^10.0.1",
    "inquirer": "^9.2.3",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@liuli-util/fs-extra": "workspace:^",
    "@types/inquirer": "^9.0.3",
    "@types/lodash-es": "^4.17.7",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.1.4",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

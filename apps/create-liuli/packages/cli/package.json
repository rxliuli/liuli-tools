{
  "name": "create-lib-template-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "{{dirName}}": "./bin.js"
  },
  "scripts": {
    "init": "pnpm build",
    "build": "tsup src/bin.ts --format esm --target esnext --sourcemap --clean",
    "dev": "pnpm build --watch",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.1.4",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "inquirer": "^9.2.3",
    "lodash-es": "^4.17.21"
  }
}

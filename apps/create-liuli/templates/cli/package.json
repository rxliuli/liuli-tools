  "bin": "./bin.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  }
}

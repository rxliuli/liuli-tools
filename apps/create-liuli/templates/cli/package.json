{
  "name": "create-lib-template-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "{{dirName}}": "./bin.js"
  },
  "scripts": {
    "init": "pnpm build",
    "build": "tsup src/bin.ts --format esm --target esnext --sourcemap --clean",
    "dev": "pnpm build --watch",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "rimraf": "^3.0.2",
    "tsup": "^6.3.0",
    "typescript": "^4.6.3",
    "vitest": "^0.24.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "inquirer": "^9.1.4",
    "lodash-es": "^4.17.21"
  }
}

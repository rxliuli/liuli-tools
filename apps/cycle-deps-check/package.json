{
  "name": "@liuli-util/cycle-deps-check",
  "version": "0.3.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "bin": {
    "cycle-deps-check": "./bin.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext",
    "dev": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext --watch",
    "start": "tsx src/bin.ts"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@liuli-util/async": "workspace:^3.3.0",
    "@liuli-util/fs-extra": "workspace:^",
    "commander": "^8.2.0",
    "enquirer": "^2.3.6",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.0",
    "lodash-es": "^4.17.21",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.8",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "type-fest": "^2.3.4",
    "typescript": "^4.5.5",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sync": [
    "jest"
  ]
}

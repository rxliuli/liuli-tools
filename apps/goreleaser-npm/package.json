    "@liuli-util/shell": "^0.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.6",
    "@types/node-fetch": "^2.5.11",
    "@types/tar": "^4.0.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",

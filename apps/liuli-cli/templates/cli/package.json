{
  "name": "@liuli-util/cli-template",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && liuli-cli build cli",
    "dev": "liuli-cli build cli -w",
    "start": "ts-node src/bin.ts",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e / -a"
  },
  "bin": {
    "cli-name": "dist/bin.js"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "inquirer": "^8.1.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "@types/inquirer": "^7.3.1",
    "@liuli-util/cli": "^3.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.6",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "type-fest": "^1.2.1",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4"
  }
}

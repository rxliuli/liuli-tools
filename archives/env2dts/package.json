{
  "name": "@liuli-util/env2dts",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "src/index.ts",
  "scripts": {
    "setup": "pnpm build",
    "build": "rimraf dist && liuli-cli build cli",
    "dev": "liuli-cli build cli -w",
    "start": "esno src/bin.ts",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e / -a"
  },
  "bin": "./bin.js",
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "commander": "^8.2.0",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "glob-promise": "^4.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@liuli-util/eslint-config-ts": "^0.1.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.6",
    "esno": "^0.9.1",
    "jest": "^28.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typedoc": "^0.21.9",
    "typescript": "^4.5.5"
  },
  "sync": [
    "gitignore",
    "eslint-ts"
  ],
  "eslintConfig": {
    "extends": [
      "@liuli-util/eslint-config-ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

{
  "name": "@liuli-util/yarn2-migration",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "setup": "saki build cli",
    "build": "rimraf dist && liuli-cli build cli",
    "dev": "liuli-cli build cli -w",
    "start": "esno src/bin.ts",
    "docs-server": "live-server docs",
    "docs-dev": "typedoc --watch",
    "docs-build": "rimraf docs && typedoc",
    "docs-deploy": "yarn docs-build && gh-pages -d docs/ -e / -a",
    "i18n-gen": "i18next-dts-gen --dirs src/i18n"
  },
  "bin": "./bin.js",
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/yarn2-migration"
  },
  "dependencies": {
    "@liuli-util/i18next-util": "^0.2.0",
    "commander": "^8.2.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "ora": "^6.0.1",
    "os-locale": "^6.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@liuli-util/i18next-dts-gen": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.6",
    "@types/yaml": "^1.9.7",
    "esno": "^0.9.1",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "sync": [
    "gitignore",
    "jest"
  ]
}

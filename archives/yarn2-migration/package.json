{
  "name": "@liuli-util/yarn2-migration",
  "version": "0.1.1",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && liuli-cli build cli",
    "dev": "liuli-cli build cli -w",
    "start": "esno src/bin.ts",
    "docs-server": "live-server docs",
    "docs-dev": "typedoc --watch",
    "docs-build": "rimraf docs && typedoc",
    "docs-deploy": "yarn docs-build && gh-pages -d docs/ -e / -a",
    "i18n-gen": "i18next-dts-gen --dirs src/i18n"
  },
  "bin": "./bin.js",
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/yarn2-migration"
  },
  "dependencies": {
    "@liuli-util/i18next-util": "workspace:^0.2.0",
    "commander": "^9.4.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "os-locale": "^6.0.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@liuli-util/i18next-dts-gen": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.4",
    "@types/yaml": "^1.9.7",
    "esno": "^0.16.3",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "sync": [
    "gitignore",
    "jest"
  ]
}

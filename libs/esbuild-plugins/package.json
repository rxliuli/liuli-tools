{
  "name": "@liuli-util/esbuild-plugins",
  "version": "0.2.0",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run"
  },
  "peerDependencies": {
    "esbuild": "^0.14.39"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "esbuild": "^0.14.53",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "tsup": "^6.2.3",
    "vitest": "^0.23.4"
  },
  "sync": [
    "jest"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "utility-types": "^3.10.0",
    "@liuli-util/fs-extra": "workspace:^"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module"
}

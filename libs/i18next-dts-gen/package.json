{
  "name": "@liuli-util/i18next-dts-gen",
  "version": "0.2.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn start && yarn pub",
    "build": "rm dist && liuli-cli build cli",
    "start": "ts-node src/bin.ts --input src/i18n",
    "pub": "yarn build && lerna clean -y && yarn",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rm docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e /i18next-dts-gen -a"
  },
  "bin": {
    "i18next-dts-gen": "dist/bin.js"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/i18next-dts-gen"
  },
  "dependencies": {
    "@liuli-util/array": "^3.2.3",
    "@liuli-util/async": "^3.2.3",
    "@liuli-util/tree": "^3.2.3",
    "chokidar": "^3.5.2",
    "commander": "^7.2.0",
    "fs-extra": "^10.0.0",
    "i18next": "^19.8.4",
    "luxon": "^1.25.0",
    "ora": "^5.4.0",
    "os-locale": "^5.0.0",
    "ts-morph": "^11.0.0"
  },
  "devDependencies": {
    "@liuli-util/cli": "*",
    "@liuli-util/shell": "*",
    "@liuli-util/test": "^3.2.3",
    "@types/fs-extra": "^9.0.11",
    "@types/luxon": "^1.25.0",
    "@types/node": "^12.20.7",
    "@types/prettier": "^2.3.0",
    "dts-minify": "^0.2.3",
    "gh-pages": "^3.2.2",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "type-fest": "^1.2.1",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4"
  }
}

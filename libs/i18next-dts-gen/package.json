{
  "name": "@liuli-util/i18next-dts-gen",
  "version": "0.3.3",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn start && yarn build",
    "build": "rimraf dist && liuli-cli build cli",
    "dev": "rimraf dist && liuli-cli build cli -w",
    "start": "esno src/bin.ts --dirs src/i18n",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e /i18next-dts-gen -a"
  },
  "bin": "./bin.js",
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/i18next-dts-gen"
  },
  "dependencies": {
    "@liuli-util/array": "^3.2.7",
    "@liuli-util/async": "^3.2.7",
    "chokidar": "^3.5.2",
    "commander": "^8.2.0",
    "fs-extra": "^10.0.0",
    "i18next": "^21.0.1",
    "os-locale": "^4.0.0",
    "ts-morph": "^12.0.0"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@liuli-util/test": "^3.2.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/prettier": "^2.3.2",
    "dts-minify": "^0.2.3",
    "esno": "^0.9.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "type-fest": "^2.3.4",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  }
}

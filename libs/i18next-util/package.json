{
  "name": "@liuli-util/i18next-util",
  "version": "0.4.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/i18next-util"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "docs:build": "rm docs && typedoc",
    "docs:deploy": "pnpm build && gh-pages -d docs/ -e / -a",
    "docs:dev": "typedoc --watch",
    "docs:server": "live-server docs",
    "gen-i18n": "i18next-dts-gen --dirs src/__tests__/i18n",
    "lint": "eslint --fix src/**",
    "setup": "pnpm build",
    "test": "vitest run"
  },
  "peerDependencies": {
    "i18next": "^20.3.3"
  },
  "devDependencies": {
    "i18next": "^22.4.15",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

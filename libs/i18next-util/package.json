{
  "name": "@liuli-util/i18next-util",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn build",
    "build": "rimraf dist && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rm docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e / -a",
    "gen-i18n": "i18next-dts-gen --dirs src/__tests__/i18n"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/i18next-util"
  },
  "peerDependencies": {
    "i18next": "^20.3.3"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.10.1",
    "@types/jest": "^27.0.2",
    "jest": "^27.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "type-fest": "^2.3.4",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  }
}

{
  "name": "@liuli-util/i18next-util",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "pnpm build",
    "build": "liuli-cli build lib",
    "dev": "liuli-cli build lib -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rm docs && typedoc",
    "docs:deploy": "pnpm build && gh-pages -d docs/ -e / -a",
    "gen-i18n": "i18next-dts-gen --dirs src/__tests__/i18n",
    "lint": "eslint --fix src/**"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/i18next-util"
  },
  "peerDependencies": {
    "i18next": "^20.3.3"
  },
  "devDependencies": {
    "i18next": "^20.3.3",
    "@liuli-util/cli": "workspace:*",
    "@types/jest": "^27.0.2",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}

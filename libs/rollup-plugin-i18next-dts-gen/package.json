{
  "name": "@liuli-util/rollup-plugin-i18next-dts-gen",
  "version": "0.4.3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "pnpm docs:build && gh-pages -d docs/ -e / -a",
    "docs:dev": "typedoc --watch",
    "docs:server": "live-server docs",
    "lint": "eslint --fix src/**",
    "setup": "pnpm build",
    "test": "vitest run"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@liuli-util/i18next-dts-gen": "workspace:^"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "tsup": "^6.2.3",
    "type-fest": "^2.18.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sync": []
}

{
  "name": "@liuli-util/rollup-plugin-i18next-dts-gen",
  "version": "0.1.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "pnpm build",
    "build": "rimraf dist && liuli-cli build lib",
    "dev": "liuli-cli build lib -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "pnpm docs:build && gh-pages -d docs/ -e / -a",
    "lint": "eslint --fix src/**"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@types/jest": "^27.0.2",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "ts-jest": "^27.1.3",
    "type-fest": "^2.3.4",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "sync": [],
  "dependencies": {
    "@liuli-util/i18next-dts-gen": "^0.3.3"
  }
}

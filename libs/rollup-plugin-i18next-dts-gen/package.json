{
  "name": "@liuli-util/rollup-plugin-i18next-dts-gen",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "pnpm build",
    "build": "liuli-cli build lib",
    "dev": "liuli-cli build lib -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "pnpm docs:build && gh-pages -d docs/ -e / -a",
    "lint": "eslint --fix src/**",
    "test": "jest --all"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "ts-jest": "^28.0.7",
    "type-fest": "^2.18.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "sync": [],
  "dependencies": {
    "@liuli-util/i18next-dts-gen": "workspace:^"
  }
}

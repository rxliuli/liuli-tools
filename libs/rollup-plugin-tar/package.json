{
  "name": "@liuli-util/rollup-plugin-tar",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn docs:build && gh-pages -d docs/ -e /api/rollup-plugin-tar -a"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "rollup": "^2.52.2"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.7.0",
    "@types/jest": "^26.0.23",
    "@types/tar": "^4.0.5",
    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "type-fest": "^1.2.1",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4"
  },
  "sync": [
    "gitignore",
    "jest"
  ],
  "dependencies": {
    "glob-promise": "^4.2.0",
    "tar": "^6.1.11"
  }
}

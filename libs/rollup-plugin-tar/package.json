{
  "name": "@liuli-util/rollup-plugin-tar",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "setup": "liuli-cli build lib",
    "build": "rimraf dist && liuli-cli build lib",
    "dev": "liuli-cli build lib -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn docs:build && gh-pages -d docs/ -e /api/rollup-plugin-tar -a"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "rollup": "^2.52.2"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/tar": "^4.0.5",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "sync": [
    "gitignore",
    "jest"
  ],
  "dependencies": {
    "glob-promise": "^4.2.0",
    "i18next": "^21.0.1",
    "tar": "^6.1.11"
  }
}

{
  "name": "@liuli-util/rollup-plugin-ts-alias",
  "version": "0.2.0",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "npm run build",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint --fix src/**",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@liuli-util/fs-extra": "workspace:^"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "typescript": "^4.7.4",
    "tsup": "^6.2.3",
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "rollup": "^2.63.0"
  },
  "sync": [
    "jest"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module"
}

{
  "name": "@liuli-util/rollup-plugin-ts-alias",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "npm run build",
    "build": "liuli-cli build lib",
    "dev": "liuli-cli build lib -w",
    "lint": "eslint --fix src/**",
    "test": "jest --all"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@liuli-util/cli": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "rollup": "^2.63.0"
  },
  "sync": [
    "jest"
  ],
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "type": "module"
}

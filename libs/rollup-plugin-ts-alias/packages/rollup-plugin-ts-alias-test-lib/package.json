  "devDependencies": {
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

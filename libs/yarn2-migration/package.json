{
  "name": "@liuli-util/yarn2-migration",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn i18n:gen",
    "build:cli": "esbuild src/bin.ts --bundle --outfile=dist/bin.js --platform=node --banner:js=\"#!/usr/bin/env node\n\"",
    "build": "rimraf dist && liuli-cli build pkg && yarn build:cli",
    "dev": "liuli-cli build pkg -w",
    "start": "esno src/bin.ts",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rimraf docs && typedoc",
    "docs:deploy": "yarn docs:build && gh-pages -d docs/ -e / -a",
    "i18n:gen": "i18next-dts-gen --dirs src/i18n"
  },
  "bin": "dist/bin.js",
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/liuli-tools/tree/master/libs/yarn2-migration"
  },
  "dependencies": {
    "@liuli-util/i18next-util": "^0.2.0",
    "commander": "^8.2.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.5",
    "lodash": "^4.17.21",
    "ora": "^6.0.1",
    "os-locale": "^6.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.11.1",
    "@liuli-util/i18next-dts-gen": "^0.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.6",
    "@types/yaml": "^1.9.7",
    "esbuild": "^0.12.29",
    "esno": "^0.9.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "ts-jest": "^27.0.5",
    "type-fest": "^2.3.4",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "sync": [
    "gitignore",
    "jest"
  ]
}

{
  "name": "@liuli-util/cycle-deps-check",
  "version": "0.3.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "bin": {
    "cycle-deps-check": "./bin.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext",
    "dev": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext --watch",
    "start": "tsx src/bin.ts"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@liuli-util/async": "workspace:^3.3.0",
    "@liuli-util/fs-extra": "workspace:^",
    "commander": "^10.0.1",
    "enquirer": "^2.3.6",
    "glob": "^10.2.4",
    "glob-promise": "^6.0.2",
    "lodash-es": "^4.17.21",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.1.4",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "type-fest": "^3.10.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

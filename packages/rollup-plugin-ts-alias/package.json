{
  "name": "@liuli-util/rollup-plugin-ts-alias",
  "version": "0.3.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --shims",
    "dev": "tsup src/index.ts --format esm,cjs --dts --shims --watch",
    "lint": "eslint --fix src/**",
    "setup": "npm run build",
    "test": "vitest run"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@liuli-util/fs-extra": "workspace:^"
  },
  "peerDependencies": {
    "rollup": "^2.63.0"
  },
  "devDependencies": {
    "@liuli-util/async": "workspace:^",
    "@liuli-util/rollup-plugin-ts-alias-test-lib": "workspace:^",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/node": "^20.1.4",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
